{
  "name": "gethired-code-tracker",
  "displayName": "Get Hired Code Tracker",
  "description": "Track the time in your projects you are working on",
  "publisher": "GetHiredCodeTracker",
  "author": {
    "name": "GetHiredCodeTracker"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "code time",
    "productivity",
    "time",
    "tracking",
    "tracker"
  ],
  "icon": "images/get-hired-logo-128.png",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:gethired-vscode-plugin.enterToken",
    "onCommand:gethired-vscode-plugin.enterTokenHard"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gethired-vscode-plugin.enterToken",
        "category": "GetHired Tracker",
        "title": "Enter Token"
      },
      {
        "command": "gethired-vscode-plugin.enterTokenHard",
        "category": "GetHired Tracker",
        "title": "Enter Token Hard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.17.27",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "node-fetch": "^3.2.0",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
